{
  "name": "@neon/data-model",
  "version": "0.2.0",
  "description": "Database schema and data models for NeonHub AI Marketing Ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\" \"prisma/**/*.prisma\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\" \"prisma/**/*.prisma\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^30.0.2",
    "prettier": "^3.6.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.19.1",
    "ts-jest": "^29.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "peerDependencies": {
    "@prisma/client": "^5.19.1"
  },
  "keywords": [
    "prisma",
    "database",
    "schema",
    "neonhub",
    "marketing",
    "ai"
  ],
  "author": "NeonHub Team",
  "license": "MIT"
}
